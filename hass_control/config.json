{
    "name": "zetao hass test",
    "version": "2.0.0",
    "slug": "zetao_hass_test",
    "description": "hass test",
    "uart": true,
    "udev": true,
    "url": "https://github.com/danyinhao/homeassistant-addon/tree/main/hass_control",
    "startup": "application",
    "boot": "auto",
    "arch": [
        "armv7"
    ],
    "map": [
        "data:rw",
        "config:rw",
        {
            "type": "share",
            "read_only": false
        },
        {
            "type": "homeassistant_config",
            "read_only": false,
            "path": "/config"
        },
        {
            "type": "addon_config",
            "read_only": false,
            "path": "/addon_config"
        }
    ],
    "options": {
        "data_path": "/config/hass-test",
        "mqtt": {
            "url": "mqtt://homeassistant.local:1883",
            "username": "",
            "password": ""
        }
    },
    "environment": {
        "mqtt_url": "{{ options.mqtt.url }}",
        "mqtt_username": "{{ options.mqtt.username }}",
        "mqtt_password": "{{ options.mqtt.password }}"
    },
    "schema": {
        "data_path": "str",
        "mqtt": {
            "url": "str?",
            "username": "str?",
            "password": "str?"
        }
    },
    "image": "ghcr.io/danyinhao/hass-test-{arch}",
    "devices": [
        "/dev/ttyS3"
    ]
}