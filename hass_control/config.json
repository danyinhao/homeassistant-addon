{
    "name": "zetao hass test",
    "version": "1.0.0",
    "slug": "zetao_hass_test",
    "description": "hass test",
    "uart": true,
    "udev": true,
    "url": "https://github.com/danyinhao/homeassistant-addon/tree/main/hass_control",
    "startup": "application",
    "boot": "auto",
    "map": [
        "config:rw"
    ],
    "options": {
        "data_path": "/config/hass-test",
        "mqtt": {
            "url": "mqtt://homeassistant.local:1883",
            "username": "",
            "password": ""
        }
    },
    "environment": {
        "mqttUrl": "{{ options.mqtt.url }}",
        "mqttUsername": "{{ options.mqtt.username }}",
        "mqttPassword": "{{ options.mqtt.password }}"
    },
    "schema": {
        "data_path": "str",
        "mqtt": {
            "url": "str?",
            "username": "str?",
            "password": "str?"
        }
    },
    "devices": [
        "/dev/ttyS3"
    ]
}