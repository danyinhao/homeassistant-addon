{
  "name": "Matter Bridge for iHost2",
  "version": "1.3.71",
  "slug": "ihost_matter_bridge2",
  "description": "Matter Bridge add-on exposes Home Assistant devices as Matter-enabled devices, enabling them to be integrated with Matter platforms, such as Apple Home, Google Home, and Amazon Alexa.",
  "url": "https://github.com/iHost-Open-Source-Project/hassio-ihost-addon/tree/master/hassio-ihost-sonoff-dongle-flasher",
  "startup": "application",
  "boot": "auto",
  "arch": [
    "armv7"
  ],
  "init": false,
  "panel_icon": "mdi:memory-arrow-down",
  "panel_title": "Matter Bridge for iHost2",
  "ingress": true,
  "ingress_port": 3030,
  "ingress_stream": true,
  "host_network": true,
  "homeassistant_api": true,
  "hassio_role": "admin",
  "hassio_api": true,
  "full_access": true,
  "apparmor": false,
  "privileged": [
    "SYS_ADMIN",
    "SYS_MODULE",
    "SYS_NICE",
    "SYS_PTRACE",
    "SYS_RAWIO",
    "SYS_RESOURCE",
    "SYS_TIME"
  ],
  "services": [
    "mqtt:need"
  ],
  "map": [
    {
      "type": "share",
      "read_only": false
    },
    {
      "type": "homeassistant_config",
      "read_only": false,
      "path": "/config"
    }
  ],
  "options": {
    "mqtt": {}
  },
  "schema": {
    "mqtt": {
      "server": "str?",
      "username": "str?",
      "password": "str?"
    }
  },
  "image": "ghcr.io/danyinhao/matter-bridge-test-{arch}",
  "devices": [
    "/dev/teepriv0",
    "/dev/tee0",
    "/dev/vendor_storage"
  ],
  "kernel_modules": true,
  "devicetree": true
}