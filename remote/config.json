{
  "name": "remote test",
  "version": "1.0.1",
  "slug": "ihost_remote_test",
  "description": "",
  "url": "https://github.com/iHost-Open-Source-Project/hassio-ihost-addon/tree/master/hassio-ihost-sonoff-dongle-flasher",
  "startup": "application",
  "boot": "auto",
  "arch": [
    "armv7"
  ],
  "init": false,
  "panel_icon": "mdi:alpha-m-circle",
  "panel_title": "Matter Bridge for iHost2",
  "ingress": true,
  "ingress_port": 3031,
  "ingress_stream": true,
  "host_network": true,
  "homeassistant_api": true,
  "hassio_role": "admin",
  "hassio_api": true,
  "full_access": true,
  "apparmor": false,
  "privileged": [
    "SYS_ADMIN",
    "SYS_MODULE",
    "SYS_NICE",
    "SYS_PTRACE",
    "SYS_RAWIO",
    "SYS_RESOURCE",
    "SYS_TIME"
  ],
  "services": [
    "mqtt:need"
  ],
  "map": [
    {
      "type": "share",
      "read_only": false
    },
    {
      "type": "homeassistant_config",
      "read_only": false,
      "path": "/config"
    }
  ],
  "options": {
    "log_level": "Info",
    "mqtt": {}
  },
  "schema": {
    "log_level": "list(None|Info|Debug)?",
    "mqtt": {
      "server": "str?",
      "username": "str?",
      "password": "str?"
    }
  },
  "image": "ghcr.io/danyinhao/remote-test-{arch}"
}